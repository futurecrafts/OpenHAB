/* Lights */
Color Light_GF_Living "LED Color"               (GF_Living, Lights)
String LightColor_GF_Living                     (GF_Living, Lights) {mqtt=">[ybmosquitto:home/esp/node01/color:command:*:default]"}
Number LightAnimation_GF_Living "LED Animation" (GF_Living, Lights) {mqtt=">[ybmosquitto:home/esp/node01/animation:command:*:default]"}

/* RF433 */
Switch Lamp_GF_Kitchen "Lamp Kitchen" (GF_Kitchen, RF433) {mqtt=">[ybmosquitto:home/esp/node03/rf433:command:on:ON],>[ybmosquitto:home/esp/node03/rf433:command:off:OFF], <[ybmosquitto:home/esp/node03/rf433:state:default]"}

/* Servo */
Dimmer ServoVer_FF_Living "ServoVer [%d %%]" <slider> (FF_Living, Servo) {mqtt=">[ybmosquitto:home/esp/node02/servo/ver:command:*:default]"}
Dimmer ServoHor_FF_Living "ServoHor [%d %%]" <slider> (FF_Living, Servo) {mqtt=">[ybmosquitto:home/esp/node02/servo/hor:command:*:default]"}

Dimmer ServoVer_SF_Living "ServoVer [%d %%]" <slider> (SF_Living, Servo) {mqtt=">[ybmosquitto:home/esp/node02-2/servo/ver:command:*:default]"}
Dimmer ServoHor_SF_Living "ServoHor [%d %%]" <slider> (SF_Living, Servo) {mqtt=">[ybmosquitto:home/esp/node02-2/servo/hor:command:*:default]"}

Number ServoMove "Servo Move" (SF_Corridor, Servo) {mqtt=">[ybmosquitto:home/esp/node02-3/servo:command:*:default]"}

/* Temp and Humid */
Number RF_Temp_OS_Main "Outside Temperature [%.1f C]" (OS_Main, Temps) {mqtt="<[ybmosquitto:home/rfm_gw/nb/node02/dev48:state:default]"}
Number RF_Hum_OS_Main "Outside Humidity [%.1f]" (OS_Main, Humids) {mqtt="<[ybmosquitto:home/rfm_gw/nb/node02/dev49:state:default]"}
Number RFCommon_Temp_OS_Main "Outside Temperature [%.1f C]" (OS_Main, Temps) {mqtt="<[ybmosquitto:home/rfm_gw/nb/node03/dev48:state:default]"}
Number RFCommon_Hum_OS_Main "Outside Humidity [%.1f]" (OS_Main, Humids) {mqtt="<[ybmosquitto:home/rfm_gw/nb/node03/dev49:state:default]"}

/* Gas and Smoke */
Number RFCommon_Gas_OS_Main "Gas [%.1f]" (OS_Main, Gas) {mqtt="<[ybmosquitto:home/rfm_gw/nb/node03/dev50:state:default]"}

/* Flame */
Number RFCommon_Flame_OS_Main "Flame [%.1f]" (OS_Main, Flame) {mqtt="<[ybmosquitto:home/rfm_gw/nb/node03/dev51:state:default]"}

/* Light */
Number RFCommon_Light_OS_Main "Light Sensor [%.1f]" (OS_Main, Light) {mqtt="<[ybmosquitto:home/rfm_gw/nb/node03/dev52:state:default]"}

/* PIR */
