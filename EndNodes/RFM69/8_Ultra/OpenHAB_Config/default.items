/* GPS */
Number GPS_lat "GPS lat [%.5f]" (OS_Front, GPS) {mqtt="<[ybmosquitto:home/rfm_gw/nb/node07/dev62:state:default]"}
Number GPS_long "GPS long [%.5f]" (OS_Front, GPS) {mqtt="<[ybmosquitto:home/rfm_gw/nb/node07/dev63:state:default]"}
String Yulia_GPS "Yulia_GPS string [%s]" {mqtt="<[ybmosquitto:yuliagps:state:JS(mqttitude-coordinates.js)]"}
Switch Yulia_GPS_alm_enb "Yulia Escape Alarm Enable"
Switch Yulia_GPS_alm_sta "Yulia Escape Alarm Status"

/* Status */
Switch RF_Temp_OS_Main_STS "Outside Temp Alarm Status"
Switch RFCommon_Gas_OS_Main_STS "Gas / Smoke Alarm Status"
